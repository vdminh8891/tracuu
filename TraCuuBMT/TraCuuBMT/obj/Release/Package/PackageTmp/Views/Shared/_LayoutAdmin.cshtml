                            <li><a href="@Url.Action("TransactionUser","Admin")"><i class="fa fa-arrow-circle-right"></i> Báo cáo thống kê</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="javascript:void(0)">

                            <i class="fa fa-user"></i> <span>User</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-red">4</small>
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("ListNormalUser", "Admin")"><i class="fa fa-arrow-circle-right"></i> Danh sách user thường</a></li>
                            <li><a href="@Url.Action("CreateNormalUser", "Admin")"><i class="fa fa-arrow-circle-right"></i> Tạo mới user thường</a></li>
                            <li><a href="@Url.Action("ListAdminUser", "Admin")"><i class="fa fa-arrow-circle-right"></i> Danh sách user admin</a></li>
                            <li><a href="@Url.Action("CreateAdminUser", "Admin")"><i class="fa fa-arrow-circle-right"></i> Tạo mới user admin</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="javascript:void(0)">

                            <i class="fa fa-user"></i> <span>Giao dịch</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-red">4</small>
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("ListTransaction", "Admin")"><i class="fa fa-arrow-circle-right"></i> Danh sách giao dịch</a></li>
                            @* <li><a href="@Url.Action("CreateTransaction", "Admin")"><i class="fa fa-arrow-circle-right"></i> Tạo mới giao dịch</a></li>*@
                        </ul>
                    </li>

                    <li class="treeview">
                        <a href="javascript:void(0)">

                            <i class="fa fa-user"></i> <span>Gói tài nguyên</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-red">4</small>
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("ListPackage", "Admin")"><i class="fa fa-arrow-circle-right"></i> Danh sách gói tài nguyên</a></li>
                            <li><a href="@Url.Action("CreatePackage", "Admin")"><i class="fa fa-arrow-circle-right"></i> Tạo mới gói tài nguyên</a></li>
                        </ul>
                    </li>

                    <li class="treeview">
                        <a href="javascript:void(0)">

                            <i class="fa fa-user"></i> <span>Biểu thuế - Thuế suất</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-red">2</small>
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("ListBieuThue", "Admin")"><i class="fa fa-arrow-circle-right"></i> Danh sách biểu thuế</a></li>
                            <li><a href="@Url.Action("CreateBieuThue", "Admin")"><i class="fa fa-arrow-circle-right"></i> Tạo mới biểu thuế</a></li>
                        </ul>
                    </li>

                    <li class="treeview">
                        <a href="javascript:void(0)">

                            <i class="fa fa-user"></i> <span>Kết quả PTPL</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-red">2</small>
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("ListKQPTPL", "Admin")"><i class="fa fa-arrow-circle-right"></i> Danh sách KQ-PTPL</a></li>
                            <li><a href="@Url.Action("CreateKQPTPL", "Admin")"><i class="fa fa-arrow-circle-right"></i> Tạo mới KQ-PTPL</a></li>
                        </ul>
                    </li>
                </ul>
            </section>
        </aside>
        <div class="content-wrapper containerAdmin" style="min-height: 956px;">
            <!-- Main content -->
            @if (TempData["WarningMessage"] != null)
            {
            <div class="alert alert-warning">
                <strong>Warning!</strong> @TempData["WarningMessage"].ToString()
            </div>
            }

            @if (TempData["ErrorMessage"] != null)
            {
                <div class="alert alert-danger">
                    <strong>Error!</strong> @TempData["ErrorMessage"].ToString()
                </div>
            }

            @if (TempData["SuccessMessage"] != null)
            {
                <div class="alert alert-success">
                    <strong>Success!</strong> @TempData["SuccessMessage"].ToString()
                </div>
            }
            @RenderBody()

        </div>

        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0.0
            </div>
            <strong>Copyright © 2021 <a href="javascript:void(0)">Tra cứu</a>.</strong> All rights reserved.
        </footer>

        <div class="control-sidebar-bg"></div>
    </div>

    <script src="@Url.Content("~/Content/js/jquery-3.6.0.min.js")"></script>
    <script src="@Url.Content("~/Content/js/fastclick.js")"></script>
    <script src="@Url.Content("~/Content/js/admin.min.js")"></script>
    <script src="@Url.Content("~/Content/js/demo.js")"></script>
    <script src="@Url.Content("~/Content/js/select2.full.min.js")"></script>
    <script src="@Url.Content("~/Content/js/bootstrap-datepicker.js")"></script>
    <script src="@Url.Content("~/Content/js/my_js.js")"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
